    "telegram sdk",
    "telegram php"
  ],
  "type": "library",
  "homepage": "https://github.com/CristianOspina/ForwardDateBot",
  "license": "MIT",
  "authors": [
    {
      "name": "Cristian",
      "homepage": "https://github.com/CristianOspina"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "irazasyed/telegram-bot-sdk": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Telegram\\Bot\\": "src/"
    }
  },
  "config": {
